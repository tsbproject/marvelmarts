  "type": "module",
  "private": true,
  "scripts": {
    "build": "npx prisma generate && next build --turbopack",
    "dev": "next dev --webpack",
    "start": "next start",
    "lint": "eslint",
    "prisma:migrate": "prisma migrate dev",
    "postinstall": "prisma generate",
